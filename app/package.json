    "version": "2.0.0",
    "description": "dbp learning Internal use system",
    "main": "index.js",
    "scripts": {
        "start": "npm run devServer & npm run devClient",
        "start-t": "TOOL=true npm start",
        "push": "cross-env NODE_ENV=production webpack && node server.js",
        "push-c": "rimraf publish && npm run push",
        "devServer": "cross-env NODE_ENV=development supervisor -i './source,./webpack,webpack.config.js' server.js",
        "devClient": "cross-env NODE_ENV=development supervisor -w './devServer.js,./webpack,webpack.config.js,./useResources.js' devServer.js",
        "getApi": "cd './api' && node getApi.js"
    },
